<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.example.com/university#"
     xml:base="http://www.example.com/university"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    
    <owl:Ontology rdf:about="http://www.example.com/university"/>
    
    <owl:ObjectProperty rdf:about="http://www.example.com/university#enrolledIn">
        <rdfs:domain rdf:resource="http://www.example.com/university#Student"/>
        <rdfs:range rdf:resource="http://www.example.com/university#Course"/>
        <rdfs:label>enrolled in</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.example.com/university#teaches">
        <rdfs:domain rdf:resource="http://www.example.com/university#Lecturer"/>
        <rdfs:range rdf:resource="http://www.example.com/university#Course"/>
        <rdfs:label>teaches</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:DatatypeProperty rdf:about="http://www.example.com/university#hasName">
        <rdfs:domain rdf:resource="http://www.example.com/university#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>has name</rdfs:label>
    </owl:DatatypeProperty>
    
    <owl:Class rdf:about="http://www.example.com/university#Course">
        <rdfs:label>Course</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.example.com/university#Lecturer">
        <rdfs:subClassOf rdf:resource="http://www.example.com/university#Person"/>
        <rdfs:label>Lecturer</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.example.com/university#Person">
        <rdfs:label>Person</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.example.com/university#Student">
        <rdfs:subClassOf rdf:resource="http://www.example.com/university#Person"/>
        <rdfs:label>Student</rdfs:label>
    </owl:Class>

</rdf:RDF>