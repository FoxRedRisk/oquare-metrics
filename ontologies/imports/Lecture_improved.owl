<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.example.com/university_v2#"
     xml:base="http://www.example.com/university_v2"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    
    <owl:Ontology rdf:about="http://www.example.com/university_v2">
        <rdfs:comment>A simple ontology describing a university, improved based on OQuaRE metrics.</rdfs:comment>
    </owl:Ontology>
    
    <owl:ObjectProperty rdf:about="http://www.example.com/university_v2#enrolledIn">
        <owl:inverseOf rdf:resource="http://www.example.com/university_v2#hasStudent"/>
        <rdfs:domain rdf:resource="http://www.example.com/university_v2#Student"/>
        <rdfs:range rdf:resource="http://www.example.com/university_v2#Course"/>
        <rdfs:comment>Relates a student to a course they are taking.</rdfs:comment>
        <rdfs:label>enrolled in</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.example.com/university_v2#hasStudent">
        <rdfs:domain rdf:resource="http://www.example.com/university_v2#Course"/>
        <rdfs:range rdf:resource="http://www.example.com/university_v2#Student"/>
        <rdfs:comment>Inverse of 'enrolledIn'. Relates a course to its enrolled students.</rdfs:comment>
        <rdfs:label>has student</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://www.example.com/university_v2#isTaughtBy">
        <owl:inverseOf rdf:resource="http://www.example.com/university_v2#teaches"/>
        <rdfs:domain rdf:resource="http://www.example.com/university_v2#Course"/>
        <rdfs:range rdf:resource="http://www.example.com/university_v2#StaffMember"/>
        <rdfs:comment>Inverse of 'teaches'. Relates a course to the staff member teaching it.</rdfs:comment>
        <rdfs:label>is taught by</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.example.com/university_v2#teaches">
        <rdfs:domain rdf:resource="http://www.example.com/university_v2#StaffMember"/>
        <rdfs:range rdf:resource="http://www.example.com/university_v2#Course"/>
        <rdfs:comment>Relates a staff member to a course they teach.</rdfs:comment>
        <rdfs:label>teaches</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:DatatypeProperty rdf:about="http://www.example.com/university_v2#hasCourseCode">
        <rdfs:domain rdf:resource="http://www.example.com/university_v2#Course"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The unique identifier for a course, e.g., CS101.</rdfs:comment>
        <rdfs:label>has course code</rdfs:label>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://www.example.com/university_v2#hasName">
        <rdfs:domain rdf:resource="http://www.example.com/university_v2#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>A literal string representing a person's name.</rdfs:comment>
        <rdfs:label>has name</rdfs:label>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://www.example.com/university_v2#hasStaffID">
        <rdfs:domain rdf:resource="http://www.example.com/university_v2#StaffMember"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>A unique staff identification number.</rdfs:comment>
        <rdfs:label>has staff ID</rdfs:label>
    </owl:DatatypeProperty>
    
    <owl:Class rdf:about="http://www.example.com/university_v2#Course">
        <rdfs:comment>A module of study offered by the university.</rdfs:comment>
        <rdfs:label>Course</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.example.com/university_v2#Lecturer">
        <rdfs:subClassOf rdf:resource="http://www.example.com/university_v2#StaffMember"/>
        <rdfs:comment>An academic staff member who teaches courses.</rdfs:comment>
        <rdfs:label>Lecturer</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.example.com/university_v2#Person">
        <rdfs:comment>A human being, either a student or staff member.</rdfs:comment>
        <rdfs:label>Person</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.example.com/university_v2#StaffMember">
        <rdfs:subClassOf rdf:resource="http://www.example.com/university_v2#Person"/>
        <rdfs:comment>An individual employed by the university.</rdfs:comment>
        <rdfs:label>Staff Member</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.example.com/university_v2#Student">
        <rdfs:subClassOf rdf:resource="http://www.example.com/university_v2#Person"/>
        <rdfs:comment>An individual enrolled in one or more courses.</rdfs:comment>
        <rdfs:label>Student</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.example.com/university_v2#TeachingAssistant">
        <rdfs:subClassOf rdf:resource="http://www.example.com/university_v2#StaffMember"/>
        <rdfs:subClassOf rdf:resource="http://www.example.com/university_v2#Student"/>
        <rdfs:comment>A student who also performs teaching or staff duties.</rdfs:comment>
        <rdfs:label>Teaching Assistant</rdfs:label>
    </owl:Class>
    
    <owl:NamedIndividual rdf:about="http://www.example.com/university_v2#AdvancedSemantics">
        <rdf:type rdf:resource="http://www.example.com/university_v2#Course"/>
        <hasCourseCode rdf:datatype="http://www.w3.org/2001/XMLSchema#string">CS700</hasCourseCode>
        <hasStudent rdf:resource="http://www.example.com/university_v2#AlexMiller"/>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://www.example.com/university_v2#AlexMiller">
        <rdf:type rdf:resource="http://www.example.com/university_v2#TeachingAssistant"/>
        <enrolledIn rdf:resource="http://www.example.com/university_v2#AdvancedSemantics"/>
        <teaches rdf:resource="http://www.example.com/university_v2#IntroToOWL"/>
        <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Alex Miller</hasName>
        <hasStaffID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">S002</hasStaffID>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://www.example.com/university_v2#IntroToOWL">
        <rdf:type rdf:resource="http://www.example.com/university_v2#Course"/>
        <hasCourseCode rdf:datatype="http://www.w3.org/2001/XMLSchema#string">CS101</hasCourseCode>
        <isTaughtBy rdf:resource="http://www.example.com/university_v2#ProfSmith"/>
        <hasStudent rdf:resource="http://www.example.com/university_v2#JaneDoe"/>
        <isTaughtBy rdf:resource="http://www.example.com/university_v2#AlexMiller"/>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://www.example.com/university_v2#JaneDoe">
        <rdf:type rdf:resource="http://www.example.com/university_v2#Student"/>
        <enrolledIn rdf:resource="http://www.example.com/university_v2#IntroToOWL"/>
        <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Jane Doe</hasName>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://www.example.com/university_v2#ProfSmith">
        <rdf:type rdf:resource="http://www.example.com/university_v2#Lecturer"/>
        <teaches rdf:resource="http://www.example.com/university_v2#IntroToOWL"/>
        <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Prof. Smith</hasName>
        <hasStaffID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">S001</hasStaffID>
    </owl:NamedIndividual>
</rdf:RDF>